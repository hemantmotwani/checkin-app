{
    "version": 2,
    "outputDirectory": "dist",
    "rewrites": [
      { "source": "/api/(.*)", "destination": "/service/src/index.js" },
      { "source": "/checkin", "destination": "/service/src/index.js" },
      { "source": "/dashboard", "destination": "/service/src/index.js" },
      { "source": "/dashboard.html", "destination": "/dashboard.html" },
      { "source": "/assets/(.*)", "destination": "/assets/$1" },
      { "source": "/(.*)", "destination": "/index.html" }
    ]
  }
